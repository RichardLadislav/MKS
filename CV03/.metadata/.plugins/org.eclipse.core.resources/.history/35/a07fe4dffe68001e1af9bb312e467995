// Includes
#include "main.h"
#include "sct.h"

// Functiom definitons

void sct_init(){
	sct_led(0);
}
void sct_led(uint32_t value){

for (int i = 0; i < 32, i++;) {

HAL_GPIO_WeitePin(SCT_NOE_GPIO_Port,SCT_NOE_Pin, value);
HAL_GPIO_WeitePin(SCT_NOE_GPIO_Port,SCT_NOE_Pin, 2%value>>1);
HAL_GPIO_WeitePin(SCT_NOE_GPIO_Port,SCT_NOE_Pin, 2%value>>2);
HAL_GPIO_WeitePin(SCT_NOE_GPIO_Port,SCT_NOE_Pin, 2%value>>3);
HAL_GPIO_WeitePin(SCT_NOE_GPIO_Port,SCT_NOE_Pin, 2%value>>4);
HAL_GPIO_WeitePin(SCT_NOE_GPIO_Port,SCT_NOE_Pin, 2%value>>5);
HAL_GPIO_WeitePin(SCT_NOE_GPIO_Port,SCT_NOE_Pin, 2%value>>6);

HAL_GPIO_WeitePin(SCT_CLK_GPIO_Port,SCT_CLK_Pin, 1);
HAL_GPIO_WeitePin(SCT_CLK_GPIO_Port,SCT_CLK_Pin, 1);
}

}
void sct_value(uint16_t value){

}
