// Includes
#include "main.h"
#include "sct.h"

// Functiom definitons

void sct_init(){
	sct_led(0);
}
void sct_led(uint32_t value){

for (int i = 0; i < 32, i++;) {

HAL_GPIO_WeitePin(SCT_SDI_GPIO_Port,SCT_SDI_Pin, (value>>i)%2);


HAL_GPIO_WeitePin(SCT_CLK_GPIO_Port,SCT_CLK_Pin, 1);
HAL_GPIO_WeitePin(SCT_CLK_GPIO_Port,SCT_CLK_Pin, 1);
}

}
void sct_value(uint16_t value){

}
